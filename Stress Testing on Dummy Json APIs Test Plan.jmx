        <JSR223PostProcessor guiclass="TestBeanGUI" testclass="JSR223PostProcessor" testname="SLA Checker PostProcessor">
          <stringProp name="scriptLanguage">groovy</stringProp>
          <stringProp name="parameters"></stringProp>
          <stringProp name="filename"></stringProp>
          <stringProp name="cacheKey">true</stringProp>
          <stringProp name="script">import java.util.concurrent.ConcurrentLinkedQueue
import java.util.Collections

// === SLA thresholds  ===
long P95_MAX_MS = 1200L     
double ERR_MAX  = 2.0       
double RPS_MIN  = 20.0      
double PASS_MIN = 98.0     
long WINDOW_MS  = 60_000L  

long now = System.currentTimeMillis()

// --- Create a shared rolling queue in JMeter &apos;props&apos; ---
def q = props.get(&quot;sla.samples&quot;) as ConcurrentLinkedQueue&lt;Map&gt;
if (q == null) {
  q = new ConcurrentLinkedQueue&lt;Map&gt;()
  props.put(&quot;sla.samples&quot;, q)
}

// One Time reset per JMeter engine start &gt;&gt; to avoid stale/mixed data
if (props.get(&quot;sla.reset&quot;) == null) {
  q.clear()
  props.put(&quot;sla.reset&quot;, &quot;1&quot;)
}

// --- Add the current sample ---
if (prev != null) {
  q.add([
    time: now,                        // when this sample finished
    latency: (prev.getTime() as long), // sample latency in ms
    isSuccess: prev.isSuccessful()      // true if sampler succeeded
  ])
}

// --- Remove entries older than the 60s window ---
while (!q.isEmpty() &amp;&amp; now - (q.peek()?.time as long) &gt; WINDOW_MS) {
  q.poll()
}

// --- Copy to a list and ensure there are no null/malformed entries ---
def list = new ArrayList&lt;Map&gt;(q)
list.removeIf { it == null || it.time == null || it.latency == null || it.success == null }

int n = list.size()
if (n == 0) return

// --- Calculate the error percentage and pass rate percentage
long errors = list.count { !it.success }
double errPrecentage  = (errors * 100.0) / n
double passPrecentage = 100.0 - errPct

// --- Calculate the p95 latency ---
def lats = list.collect { (it.latency as long) }
Collections.sort(lats)
long p95 = lats.get(Math.min((int)Math.ceil(0.95 * n) - 1, n - 1))

// --- Calculate the RPS over last 5 seconds ---
int hitsLast5s = list.count { now - (it.time as long) &lt;= 5000L }
double rps = hitsLast5s / 5.0

// --- SLA check ---
boolean broken =
    (p95 &gt; P95_MAX_MS) ||
    (errPct &gt;= ERR_MAX) ||
    (rps &lt; RPS_MIN) ||
    (passPct &lt; PASS_MIN)

if (broken) {
  log.warn(String.format(
    &quot;SLA BROKEN : p95=%d ms  err=%.2f%%  rps=%.2f  pass=%.2f%%  n=%d&quot;,
    p95, errPct, rps, passPct, n
  ))
  prev.setStopTest(true)
}</stringProp>
        </JSR223PostProcessor>
